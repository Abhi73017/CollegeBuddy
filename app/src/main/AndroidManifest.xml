<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.abhishek.collegebuddy">

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="CollegeBuddy"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".OldSyllabus.civilold"></activity>
        <activity android:name=".NewSyllabus.civilnew" />
        <activity android:name=".NewSyllabus.FirstSem.EEE_1st_new" />
        <activity android:name=".NewSyllabus.MechNew" />
        <activity android:name=".OldSyllabus.MechOld" />
        <activity android:name=".OldSyllabus.EeeOld" />
        <activity android:name=".NewSyllabus.EeeNew" />
        <activity
            android:name=".splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".NewSyllabus.new_syllabus_cse"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OldSyllabus.old_syllabus_cse"
            android:screenOrientation="portrait" />
        <activity android:name=".syllabus" />
        <activity
            android:name=".dashboard"
            android:label="@string/title_activity_dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".loginsignup.login"
            android:screenOrientation="portrait" />
    </application>

</manifest>