<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.collegebuddy.fragment_home"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_Contact_Us"
        android:name="com.example.collegebuddy.fragment_contact_us"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_contact_us" />

    <fragment
        android:id="@+id/navigation_My_account"
        android:name="com.example.collegebuddy.fragment_my_account"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_my_account" />

    <fragment
        android:id="@+id/navigation_More"
        android:name="com.example.collegebuddy.fragment_more"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_more" />
</navigation>